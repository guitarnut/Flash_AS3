package com.gnut.media.audioplayer.components{	import flash.events.MouseEvent;	public class MuteButton extends AudioPlayerButton	{		private var _muted:Boolean;		public function MuteButton($mc)		{			super($mc);			init();		}		private function init():void		{			_v.mc_unmute.visible = true;			addEventListener(MouseEvent.CLICK, toggleIcon);		}		private function toggleIcon(e:MouseEvent):void		{			if (_muted==true)			{				_muted = false;			}			else			{				_muted = true;			}			setIcon();		}		private function setIcon():void		{			if (_muted)			{				_v.mc_unmute.visible = true;			}			else			{				_v.mc_unmute.visible = false;			}		}		public function set muted($b:Boolean):void		{			_muted = $b;			setIcon();		}		public function get muted():Boolean		{			return _muted;		}	}}