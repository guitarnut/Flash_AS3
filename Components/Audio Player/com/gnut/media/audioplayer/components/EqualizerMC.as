package com.gnut.media.audioplayer.components{	import flash.media.*;	import flash.net.*;	import flash.display.*;	import flash.events.*;	import com.everydayflash.equalizer.*;	import com.everydayflash.equalizer.color.*;	public class EqualizerMC extends Sprite	{		public function EqualizerMC()		{			var es:EqualizerSettings = new EqualizerSettings();			es.numOfBars = 15;			es.height = 30;			es.barSize = 3;			es.vgrid = true;			es.hgrid = 3;			es.colorManager = new SolidBarColor(0xFFFFFFFF);			//es.effect = EqualizerSettings.FX_REFLECTION;			var e:Equalizer = new Equalizer();			e.alpha = .5;			e.update(es);			e.y = -1*(es.height);			e.x = 0;			e.width = 60;			addChild(e);			addEventListener(Event.ENTER_FRAME, e.render);		}	}}