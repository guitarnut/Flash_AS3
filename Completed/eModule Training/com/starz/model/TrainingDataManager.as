package com.starz.model{	import com.starz.constants.TrainingData;	public class TrainingDataManager	{		private var _sectionsCompleted:Array;		private var _passportStampsEarned:Array;		private var _quizQuestionsAnswered:Number;		private var _quizesAced:Number;		private var _totalStampsEarned:Number;		private var _currentStampsEarned:Number;		private var _locationOneQuizTotalCorrect:Number;		private var _locationTwoQuizTotalCorrect:Number;		private var _locationThreeQuizTotalCorrect:Number;		private var _locationFourQuizTotalCorrect:Number;		private var _totalQuizQuestions:Number;		private var _locationOneComplete:Boolean;		private var _locationTwoComplete:Boolean;		private var _locationThreeComplete:Boolean;		private var _locationFourComplete:Boolean;		private var _trainingComplete:Boolean;		public function TrainingDataManager()		{			init();		}		private function init():void		{			_sectionsCompleted = new Array();			_passportStampsEarned = new Array();			_quizQuestionsAnswered = 0;			_quizesAced = 0;			_totalStampsEarned = 0;			_currentStampsEarned = 0;		}		public function setTrainingData(d$:String, v$:*):void		{			switch (d$)			{				case TrainingData.PASSPORT_STAMPS :					_passportStampsEarned.push(v$);					_totalStampsEarned++;					_currentStampsEarned++;					break;				case TrainingData.SECTIONS_COMPLETED :					_sectionsCompleted.push(v$);					break;				default :					break;			}		}		public function getTrainingData(d$:String):*		{			var trainingData:*;						switch (d$)			{				case TrainingData.PASSPORT_STAMPS :					trainingData = new Object();					trainingData.stampsEarned = _passportStampsEarned;					trainingData.totalStamps = _currentStampsEarned;					break;				case TrainingData.SECTIONS_COMPLETED :					trainingData = _sectionsCompleted;					break;				default :					break;			}			return trainingData;						_currentStampsEarned = 0;		}		public function reset():void		{			init();		}	}}