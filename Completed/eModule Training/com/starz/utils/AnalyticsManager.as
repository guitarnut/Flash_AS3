package com.starz.utils{	import flash.external.ExternalInterface;	import com.starz.events.TrainingEvent;	public class AnalyticsManager	{		public function AnalyticsManager():void		{		}		public function track(e:TrainingEvent):void		{						if (ExternalInterface.available)			{				try				{					ExternalInterface.call("_gaq.push", ['_trackEvent', "Training Event", String(e.type)]);				}				catch (e:Error)				{				}			}			else			{			}		}	}}