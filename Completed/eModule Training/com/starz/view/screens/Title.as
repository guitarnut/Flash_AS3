package com.starz.view.screens{	import com.starz.view.screens.base.TrainingScreen;	import flash.events.MouseEvent;	import com.starz.events.TrainingEvent;	public class Title extends TrainingScreen	{		public function Title()		{			init();		}		private function init():void		{			b_start.addEventListener(MouseEvent.CLICK, handleEvents);		}		private function handleEvents(e:MouseEvent):void		{			nextFrame();		}		override protected function reset():void		{			fireEvent(TrainingEvent.GOTO_INTRO);		}	}}