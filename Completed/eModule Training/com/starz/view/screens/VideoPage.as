package com.starz.view.screens{	import flash.display.MovieClip;	import com.greensock.TweenLite;	import flash.events.Event;	import fl.video.VideoEvent;	import com.starz.view.screens.base.TrainingScreen;	import fl.video.FLVPlayback;	import flash.events.MouseEvent;	import com.starz.view.components.SimpleMC;	import com.starz.view.components.VideoClip;	public class VideoPage extends VideoClip	{		public function VideoPage()		{			init();		}		private function init():void		{			alpha = 0;			visible = false;			addListeners();		}		override protected function addListeners():void		{			flvVideo.addEventListener(VideoEvent.COMPLETE, hideVideo);			skipButton.addEventListener(MouseEvent.CLICK, hideVideo);		}	}}