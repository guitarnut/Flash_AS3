package com.starz.view.screens{	import com.starz.view.screens.base.TrainingScreen;	import flash.events.Event;	import flash.events.MouseEvent;	import com.starz.utils.URLNavigator;	public class Assessment extends TrainingScreen	{		public function Assessment()		{			init();		}		private function init():void		{			b_enter.addEventListener(MouseEvent.CLICK, function() {			 nextFrame();			 URLNavigator.ChangePage('http://www.starztraining.com/starz1')			 });			addEventListener(Event.ENTER_FRAME, setupContent);		}		private function setupContent(e:Event):void		{			removeEventListener(Event.ENTER_FRAME, setupContent);		}	}}