package com.starz.view.screens{	import com.starz.view.screens.base.TrainingScreen;	import flash.events.Event;	import com.starz.events.TrainingEvent;	public class Promotion extends TrainingScreen	{		public function Promotion()		{			init();		}		private function init():void		{			addEventListener(Event.ENTER_FRAME, setupContent);		}		private function setupContent(e:Event):void		{			removeEventListener(Event.ENTER_FRAME, setupContent);		}		override protected function reset():void		{			fireEvent(TrainingEvent.PROMOTION_ONE_COMPLETE);		}	}}