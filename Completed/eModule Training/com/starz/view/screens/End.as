package com.starz.view.screens{	import com.starz.view.screens.base.TrainingScreen;	import flash.events.MouseEvent;	import com.starz.events.TrainingEvent;	import com.starz.utils.URLNavigator;	import flash.events.Event;	public class End extends TrainingScreen	{		public function End()		{			init();		}		private function init():void		{			b_original1.showButton();			b_original2.showButton();			b_original3.showButton();			//b_starz.showButton();			//b_tweet.showButton();			//b_share.showButton();			b_credit.showButton();			//b_sweepstakes.showButton();			b_ipad.showButton();			b_rules.showButton();			mc_rules.b_closeRules.showButton();			b_original1.addEventListener(MouseEvent.CLICK, handleEvents);			b_original2.addEventListener(MouseEvent.CLICK, handleEvents);			b_original3.addEventListener(MouseEvent.CLICK, handleEvents);			//b_starz.addEventListener(MouseEvent.CLICK, handleEvents);			//b_tweet.addEventListener(MouseEvent.CLICK, handleEvents);			//b_share.addEventListener(MouseEvent.CLICK, handleEvents);			b_credit.addEventListener(MouseEvent.CLICK, handleEvents);			//b_sweepstakes.addEventListener(MouseEvent.CLICK, handleEvents);			b_ipad.addEventListener(MouseEvent.CLICK, handleEvents);			b_rules.addEventListener(MouseEvent.CLICK, handleEvents);			mc_rules.b_closeRules.addEventListener(MouseEvent.CLICK, handleEvents);			text_thanks.showMC();		}		private function handleEvents(e:MouseEvent):void		{			switch (e.target.name)			{				case 'b_original1' :					URLNavigator.ChangePage('http://www.starz.com/originals/magiccity');					break;				case 'b_original2' :					URLNavigator.ChangePage('http://www.starz.com/originals/TheWhiteQueen');					break;				case 'b_original3' :					URLNavigator.ChangePage('http://www.starz.com/Originals/BlackSails');					break;				case 'b_starz' :					URLNavigator.ChangePage('http://www.TWCSR.com');					break;				case 'b_tweet' :					URLNavigator.ChangePage('http://www.twitter.com');					break;				case 'b_share' :					URLNavigator.ChangePage('http://www.facebook.com');					break;				case 'b_credit' :					URLNavigator.ChangePage('http://www.starztraining.com/starz1');					break;				case 'b_sweepstakes' :					URLNavigator.ChangePage('http://www.starz.com');					break;				case 'b_ipad' :					dispatchEvent(new TrainingEvent(TrainingEvent.GOTO_ASSESSMENT, true, true));					break;				case 'b_rules' :					mc_rules.showMC();					break;				case 'b_closeRules' :					mc_rules.hideMC();					break;			}		}	}}